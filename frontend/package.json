{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.13.0"
  },
  "scripts": {
    "all-checks": "npm run lint && npm run ts:check && npm run test && npm run build -- --no-lint",
    "build": "next build",
    "dev": "NEW_RELIC_ENABLED=false next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "format": "prettier --write '**/*.{js,json,md,ts,tsx,scss,yaml,yml}'",
    "format-check": "prettier --check '**/*.{js,json,md,ts,tsx,scss,yaml,yml}'",
    "lint": "next lint",
    "lint-fix": "npm run lint -- --fix",
    "postinstall": "node ./scripts/postinstall.js",
    "start:nr": "NEW_RELIC_ENABLED=true NODE_OPTIONS='-r @newrelic/next' next start -p ${PORT:-3000}",
    "start": "next start -p ${PORT:-3000}",
    "storybook": "storybook dev -p 6006",
    "storybook-build": "storybook build",
    "test": "jest --ci --coverage",
    "test:pa11y-desktop": "pa11y-ci --config .pa11yci-desktop.json --sitemap http://localhost:3000/sitemap.xml",
    "test:pa11y-mobile": "pa11y-ci --config .pa11yci-mobile.json --sitemap http://localhost:3000/sitemap.xml",
    "test-update": "jest --update-snapshot",
    "test-watch": "jest --watch",
    "test:e2e": "npx playwright test --config ./tests/playwright.config.ts",
    "test:e2e:ui": "npx playwright test --config ./tests/playwright.config.ts --ui",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "@newrelic/next": "^0.10.0",
    "@next/third-parties": "^15.1.4",
    "@opentelemetry/api": "^1.8.0",
    "@trussworks/react-uswds": "github:trussworks/react-uswds#58f3089739ac5ee415c11cfbe179daad7a62597",
    "@uswds/uswds": "^3.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "focus-trap-react": "^10.2.3",
    "isomorphic-dompurify": "^2.15.0",
    "jose": "^5.9.6",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "newrelic": "^12.7.0",
    "next": "^15.1.4",
    "next-intl": "^3.2.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "server-only": "^0.0.1",
    "sharp": "^0.33.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@playwright/test": "^1.44.0",
    "@storybook/addon-essentials": "^8.5.2",
    "@storybook/nextjs": "^8.5.2",
    "@storybook/react": "^8.5.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest-axe": "^3.5.5",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.13",
    "@types/newrelic": "^9.14.6",
    "@types/new-relic-browser": "^1.230.5",
    "@types/node": "^22.10.5",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^19",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-nava": "^12.1.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-storybook": "^0.9.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "jest": "^29.5.0",
    "jest-axe": "^7.0.0",
    "jest-cli": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "pa11y-ci": "^3.1.0",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.1.0",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^3.2.5",
    "sass": "^1.77.0",
    "sass-loader": "^13.2.0",
    "storybook": "^8.5.2",
    "style-loader": "^3.3.2",
    "typescript": "^5.0.0"
  }
}
